<div id="cube" class="cube">
    <div class="innercube">
        <div class="gamepanelcontainer">
            <%if ( typeof lobbyresult != 'undefined' ) { %>
                <h3>Game Id: <%= lobbyresult.lobbyid %></h3>
                <p>MinBet: <%= lobbyresult.minbet%> Seats available: <%= lobbyresult.seatsavailable%></p>
            <% } %>
            
            <h3>Game Id: 1</h3>
            <p>Min Bet: x Seats available: y</p>

            <table id="gameTable" class="table table-striped">
            <tr>
            <th scope="col">UserID</th>
            <th scope="col">Username</th>
            <th scope="col">Currency</th>
            <th scope="col">Current Bet Amount</th>
            <td scope="col">Card1</td>
            <td scope="col">Card2</td>
            <td scope="col">PlayerTurn?</td>
            <td scope="col">Buttons</td>
            </tr>

            <tbody>
            <tr>
                <td>1</td>
                <td>Ted</td>
                <td>$123</td>
                <td>$1</td>
                <td>King Diamond</td>
