
<div id="cube" class="cube">
    <div class="innercube"  id="innercube" style="border: thick solid black;width: 100px; float:top; height:100px; background:#26547C; margin:10px">
        <div class="gamepanelcontainer">
            <table id="gameTable" class="table table-striped" width="800px">
                <tr>
                    <th scope="col">Game ID</th>
                    <th scope="col"># of Players</th>
                    <th scope="col">Chips Needed</th>
                    <th scope="col">Game Status</th>
                    <td scope="col">Buttons</td>
                </tr>

                <tbody>
                    <tr>
                        <td>1</td>
                        <td>3</td>
                        <td>4</td>
                        <td>open</td>
                        <td width="200px">
                            <button style="color:white;background-color:#3CB371;" type="button" onclick="" class="btn btn-secondary btn-sm">Join Game</button>
                            <button style="color:white;background-color:#3CB371;" type="button" onclick="" class="btn btn-secondary btn-sm">Watch Game</button>
                        </td>
                    </tr>
                    <tr>
                        <td>2</td>
                        <td>6</td>
                        <td>10</td>
                        <td>closed</td>
                        <td>
                            <button style="color:white;background-color:#960200;" type="button" onclick="" class="btn btn-secondary btn-sm">Join Game</button>
                            <button style="color:white;background-color:#3CB371;" type="button" onclick="" class="btn btn-secondary btn-sm">Watch Game</button>
                        </td>
                    </tr>
                </tbody>
            </table>
        </div>
    </div>
    <div class="innercubeside"  id="innercubeside" style="border: thick solid black;width: 100px; float:left; height:100px; background:26547C; margin:10px">
        <div class="profilepanelcontainer">
